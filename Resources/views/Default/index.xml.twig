<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for result in results %}
    
    <url>
        
      <loc>{{ url }}{{ path(result) }}</loc>

      <lastmod>{{ result.contentInfo.modificationDate|date('Y-m-d') }}</lastmod>
      
    </url>   
    
    {%endfor%}
    
</urlset>
